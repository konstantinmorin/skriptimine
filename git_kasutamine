#root kasutaja
install - apt install git
#user kasutaja
esmakordselt

#seadista nima, mail
git config --global user.name "Konstantin Morin"
git config --global user.email "morin.konstantin@khk.ee"
git config --global core.editor nano

#loo katalog
mkdir skriptimise_alused

#sisene antud kausta
cd skriptimise_alused

#tekita repositoorium
get init

#iga faili muudatuse korral:
get status - kontrolli et fail on muudetud
git add file_name - lisa fail jälgimissüsteemi
git commit -m = koosta kirjelduse
git log - kontrolli, et commit on salvestatud
